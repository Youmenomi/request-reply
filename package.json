{
  "name": "request-reply",
  "version": "0.0.2",
  "description": "",
  "keywords": [
    "typescript",
    "request",
    "reply",
    "response",
    "async",
    "in-app"
  ],
  "main": "dist/cjs/request-reply.cjs.js",
  "module": "dist/esm/request-reply.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "ts:check": "tsc --noEmit -p tsconfig.build.json",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly -p tsconfig.build.json",
    "build:clean": "del dist",
    "build": "npm run ts:check && eslint --ext .ts,.js ./src && npm run build:clean && npm run build:js && npm run build:types",
    "release": "np",
    "version": "npm run build"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Youmenomi/request-reply.git"
  },
  "author": "Dean Yao <youmenomi@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Youmenomi/request-reply/issues"
  },
  "homepage": "https://github.com/Youmenomi/request-reply#readme",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "np": "^7.2.0",
    "prettier": "^2.2.1",
    "rollup": "^2.36.1",
    "rollup-simple-configer": "^0.1.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "auto-bind": "^4.0.0",
    "p-map": "^4.0.0",
    "pichu": "^0.1.2"
  }
}
